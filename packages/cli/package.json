{
  "name": "@daily-paper/cli",
  "version": "0.0.0",
  "description": "Daily Paper management CLI",
  "license": "MIT",
  "author": "James Durand",
  "homepage": "https://github.com/durandj/daily-paper#readme",
  "bugs": {
    "url": "https://github.com/durandj/daily-paper/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/durandj/daily-paper.git"
  },
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build '**/*.tsbuildinfo'",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "command": "ts-node --require tsconfig-paths/register ./src/index.ts"
  },
  "dependencies": {
    "@daily-paper/core": "workspace:^0.0.0",
    "commander": "^6.2.1",
    "js-yaml": "^3.14.1",
    "pug": "^3.0.0",
    "puppeteer": "^5.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@daily-paper/eslint-config": "workspace:^0.0.0",
    "@daily-paper/tsconfig": "workspace:^0.0.0",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.14.14",
    "@types/pug": "^2.0.4",
    "@types/puppeteer": "^5.4.2",
    "eslint": "^7.15.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
