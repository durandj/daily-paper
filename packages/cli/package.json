{
  "name": "@daily-paper/cli",
  "version": "0.0.0",
  "description": "Daily Paper management CLI",
  "license": "MIT",
  "author": "James Durand",
  "homepage": "https://github.com/durandj/daily-paper#readme",
  "bugs": {
    "url": "https://github.com/durandj/daily-paper/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/durandj/daily-paper.git"
  },
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf build",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "command": "ts-node --require tsconfig-paths/register ./src/index.ts"
  },
  "dependencies": {
    "ajv": "^7.0.0-beta.7",
    "ajv-formats": "^0.5.0",
    "commander": "^6.2.0",
    "js-yaml": "^3.14.0",
    "pug": "^3.0.0",
    "puppeteer": "^5.5.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.14.9",
    "@types/pug": "^2.0.4",
    "@types/puppeteer": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
